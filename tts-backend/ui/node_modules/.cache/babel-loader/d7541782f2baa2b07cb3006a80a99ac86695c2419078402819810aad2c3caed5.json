ory] = useState('');\n    const [ssml, setSsml] = useState('');\n\n    useEffect(() => {\n        fetch('/api/categories')\n            .then(res => res.json())\n            .then(data => setCategories(data));\n    }, []);\n\n    const handleSave = () => {\n        fetch('/api/save-ssml', {\n            method: 'POST',\n            headers: { 'Content-Type': 'application/json' },\n            body: JSON.stringify({ ssml, language, category }),\n        }).then(res => res.json())\n          .then(data => alert(data.success ? 'Saved!' : 'Failed!'));\n    };\n\n    return (\n        <div>\n            <h1>SSML Configuration</h1>\n            <label>\n                Language:\n                <input value={language} onChange={e => setLanguage(e.target.value)} />\n            </label>\n            <label>\n                Category:\n                <select value={category} onChange={e => setCategory(e.target.value)}>\n                    {categories.map(cat => <option key={cat} value={cat}>{cat}</option>)}\n                </select>\n            </label>\n            <label>\n                SSML:\n                <textarea value={ssml} onChange={e => setSsml(e.target.value)} />\n            </label>\n            <button onClick={handleSave}>Save</button>\n        </div>\n    );\n}\n\nexport default App;\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,QAAQ,CAAEC,SAAS,KAAQ,OAAO,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAEnD,QAAS,CAAAC,GAAGA,CAAA,CAAG,CACX,KAAM,CAACC,UAAU,CAAEC,aAAa,CAAC,CAAGR,QAAQ,CAAC,EAAE,CAAC,CAChD,KAAM,CAACS,QAAQ,CAAEC,WAAW,CAAC,CAAGV,QAAQ,CAAC,OAAO,CAAC,CACjD,KAAM,CAACW,QAAQ,CAAEC,WAAW,CAAC,CAAGZ,QAAQ,CAAC,EAAE,CAAC,CAC5C,KAAM,CAACa,IAAI,CAAEC,OAAO,CAAC,CAAGd,QAAQ,CAAC,EAAE,CAAC,CAEpCC,SAAS,CAAC,IAAM,CACZc,KAAK,CAAC,iBAAiB,CAAC,CACnBC,IAAI,CAACC,GAAG,EAAIA,GAAG,CAACC,IAAI,CAAC,CAAC,CAAC,CACvBF,IAAI,CAACG,IAAI,EAAIX,aAAa,CAACW,IAAI,CAAC,CAAC,CAC1C,CAAC,CAAE,EAAE,CAAC,CAEN,KAAM,CAAAC,UAAU,CAAGA,CAAA,GAAM,CACrBL,KAAK,CAAC,gBAAgB,CAAE,CACpBM,MAAM,CA